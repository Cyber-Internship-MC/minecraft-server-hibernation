{
  "Server": {
    "Folder": "{path/to/server/folder}",
    "FileName": "{server.jar}",
    "Version": "1.19.2",
    "Protocol": 760
  },
  "Commands": {
    "StartServer": "java <Commands.StartServerParam> -jar <Server.FileName> nogui",
    "StartServerParam": "-Xmx4096M -Xms4096M",
    "StopServer": "stop",
    "StopServerAllowKill": -1
  },
  "Msh": {
    "Debug": 1,
    "ID": "",
    "MshPort": 25565,
    "MshPortQuery": 25565,
    "EnableQuery": true,
    "TimeBeforeStoppingEmptyServer": 120,
    "SuspendAllow": false,
    "SuspendRefresh": -1,
    "InfoHibernation": "§fserver status: §b§lHIBERNATING, enter to restart server\n§f服务器状态: §b§l休眠，进入以重启服务器",
    "InfoStarting": "§fserver status: §6§lWARMING UP\n§f服务器状态：§6§l正在启动",
    "NotifyUpdate": false,
    "NotifyMessage": true,
    "Whitelist": [],
    "WhitelistImport": false,
    "ShowResourceUsage": false,
    "ShowInternetUsage": false
  }
}